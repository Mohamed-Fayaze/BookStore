        <div class="col-lg-2">

            <form method="get" asp-controller="Healthcare" asp-action="Index">

                <input name="searchstring" type="hidden" value="@ViewData["search"]" />
                <input name="locationstring" type="hidden" value="@ViewData["location"]" />
                <input name="searchcategoryid" type="hidden" value="@ViewData["category"]" />
                <div class="row">
                    <div style="width:170px" class="rld-single-select ml-22">
                        <a asp-controller="HealthcareDoctor" asp-action="Listing" class="btn btn-outline-primary">View By Doctor List</a>
                    </div>
                    <div style="margin:10px 0px 0px 50px">
                        <p id="getlocality"></p>
                    </div>

                </div>
                <div class="row">
                    <div style="width:170px" class="rld-single-select ml-22">
                        <select class="select single-select" name="filter.District">
                            <option value="">Select Area</option>
                            @foreach (var item in Model.DistrictList)
                            {
                                if (ViewBag.district == item.Id)
                                {
                                    <option value="@item.Id" selected>@item.Name</option>
                                }
                                else
                                {
                                    <option value="@item.Id">@item.Name</option>
                                }
                            }
                        </select>
                    </div>
                </div>

                <div class="row" style="margin-top:10px">
                    <div class="rld-single-select ml-22">
                        <select class="select single-select" name="filter.Speciality" multiple>
                            <option value="">Select Speciality</option>
                            @foreach (var item in Model.SpecialityList)
                            {
                                var value = ViewData["speciality"].ToString().Split(new char[] { ',' }, StringSplitOptions.RemoveEmptyEntries);
                                if (value.Count() > 0 && Array.Exists(value, s => s == item.Id.ToString()))
                                {
                                    <option value="@item.Id" selected>@item.SpecialityName</option>
                                }
                                else
                                {
                                    <option value="@item.Id">@item.SpecialityName</option>
                                }
                            }
                        </select>
                    </div>
                </div>

                <div class="row" style="margin-top:10px">
                    <div style="width:170px" class="rld-single-select">
                        <select class="select single-select mr-0" name="filter.Facility" multiple>
                            <option value="">Select Facility</option>
                            @foreach (var item in Model.FacilityList)
                            {
                                var value = ViewData["facility"].ToString().Split(new char[] { ',' }, StringSplitOptions.RemoveEmptyEntries);
                                if (value.Count() > 0 && Array.Exists(value, s => s == item.Id.ToString()))
                                {
                                    <option value="@item.Id" selected>@item.Name</option>
                                }
                                else
                                {
                                    <option value="@item.Id">@item.Name</option>
                                }
                            }
                        </select>
                    </div>
                </div>
                
                <div class="row" style="margin-top:10px">

                    <div style="width:170px" class="rld-single-select ml-22">
                        <select class="select single-select" name="filter.Language" multiple>
                            <option value="">Select Language</option>
                            @foreach (var item in Model.LanguageList)
                            {
                                var value = ViewData["language"].ToString().Split(new char[] { ',' }, StringSplitOptions.RemoveEmptyEntries);
                                if (value.Count() > 0 && Array.Exists(value, s => s == item.Id.ToString()))
                                { 
                                    <option value="@item.Id" selected>@item.Name</option>
                                }
                                else
                                {
                                    <option value="@item.Id">@item.Name</option>
                                }
                            }
                        </select>
                    </div>
                </div>
                <div class="row" style="margin-top:10px">

                    <div style="width:170px" class="rld-single-select ml-22">
                        <input class="select single-select" value="@ViewBag.HospitalName" name="filter.HospitalName">
                    </div>
                </div>
                <div class="row" style="margin-top:10px">
                    <div class="col-xl-2 col-lg-2 col-md-4 pl-0">
                        <button class="btn btn-primary">Filter</button>
                    </div>
                </div>
            </form>
        </div>
        <div class="col-lg-10">
            <div class="padding-45px">

                <!-- Listing Search -->
                <div class="listing-search">

                    <form class="row no-gutters" method="get" asp-controller="Healthcare" asp-action="Index">

                        <input name="searchstring" type="hidden" value="@ViewData["search"]" />
                        <input name="locationstring" type="hidden" value="@ViewData["location"]" />
                        <input name="searchcategoryid" type="hidden" value="@ViewData["category"]" />
                        <div class="col-md-4 col-3">
                            <div class="keywords">
                                <input class="listing-form first" type="text" placeholder="Keywords..." name="specialistkeyword">
                            </div>
                        </div>
                        <div class="col-md-4 col-3">
                            <div class="listing-form second">
                                <select class="select single-select" name="filter.StateId">
                                    <option value="">All Location</option>
                                    @foreach (var item in Model.StateList)
                                    {
                                        if (ViewBag.state == item.Id)
                                        {
                                            <option value="@item.Id" selected>@item.Name</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Id">@item.Name</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="col-md-4 col-3">
                            <div class="listing-form">
                                <select class="select single-select" name="filter.Department">
                                    <option value="">All Department</option>
                                    @foreach (var item in Model.DepartmentList)
                                    {
                                        if (ViewBag.department == item.Id)
                                        {
                                            <option value="@item.Id" selected>@item.DepartmentName</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Id">@item.DepartmentName</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="col-md-4 col-3">
                            <button type="submit" class="listing-bottom background-dark box-shadow">Search Now</button>
                        </div>
                    </form>
</div>
                <!-- // Listing Search -->

                <hr>
                <div class="float-left">@Model.PagingInfo.TotalItem Results Found</div>
                @*<div class="float-right">
                        <a href="map-list-layout.html" class="d-inline-block margin-right-15px text-main-color"><i class="fas fa-list-ul"></i></a>
                        <a href="map-grid-layout.html"><i class="fas fa-th-large"></i></a>
                    </div>*@
                <div class="clearfix"></div>
                <hr>


                <div class="margin-tb-45px">
                    @foreach (var item in Model.Healthcares)
                    {
                        <!-- clinic -->
                        <div class="background-white thum-hover box-shadow hvr-float full-width margin-bottom-45px">
                            <div class="float-lg-left margin-right-30px sm-mr-0px text-center sm-mt-35px">
                                <a asp-area="Customer" asp-action="Detail" asp-controller="Healthcare" asp-route-id="@item.Id" class="homes-img hover-effect">
                                    
                                        @if (item.Image != null)
                                        {
                                            <img src="@item.Image" alt="home-1" class="img-responsive" style="width:18rem">
                                        }
                                        else
                                        {

                                            <img src="~/MedicalListing/assets/img/logo-1.png" alt="">
                                        }
                                       
                                    
                                </a>
                            </div>
                            <div class="padding-lr-25px padding-tb-50px">
                               
                                    <span class="text-grey-2"><i class="far fa-map"></i>  @item.District</span>
                                 
                              
                                <h2><a asp-area="Customer" asp-action="Detail" asp-controller="Healthcare" asp-route-id="@item.Id" class="d-block text-dark text-capitalize text-medium margin-tb-15px">@item.HospitalName</a></h2>
                                <div class="row no-gutters">
                                    <div class="col-4"><a href="#" class="text-lime"><i class="far fa-bookmark"></i> Open Now!</a></div>
                                    <div class="col-4 text-center"><a href="#" class="text-red"><i class="far fa-heart"></i> Save</a></div>
                                    @*<div class="col-4 text-right"><a href="#" class="text-blue"><i class="far fa-hospital"></i> @item.SubCategory.SecondaryName</a></div>*@
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <!-- // clinic -->
                    }
                </div>
                @if (Model.PagingInfo.TotalItem > 0)
                {
                    <ul class="pagination pagination-md ">
                        @if (Model.PagingInfo.CurrentPage == 1 || Model.PagingInfo.TotalPage == 1)
                        {
                            <li class="page-item disabled">
                                <a class="page-link rounded-0">Previous</a>
                            </li>
                        }
                        else
                        {
                            <li class="page-item">
                                <a class="page-link rounded-0" asp-action="Index" asp-route-ListingPage="@(Model.PagingInfo.CurrentPage - 1)">Previous</a>
                            </li>
                        }
                        @for (var val = 1; val <= Model.PagingInfo.TotalPage; val++)
                        {

                            if (val == Model.PagingInfo.CurrentPage)
                            {
                                <li class="page-item active">
                                    <a class="page-link" asp-action="Index" asp-route-ListingPage="@val">@val <span class="sr-only">(current)</span></a>
                                </li>
                            }
                            else
                            {
                                <li class="page-item"><a class="page-link" asp-action="Index" asp-route-ListingPage="@val">@val</a></li>
                            }

                        }
                        @if (Model.PagingInfo.TotalPage == 1 || Model.PagingInfo.TotalPage == Model.PagingInfo.CurrentPage)
                        {
                            <li class="page-item disabled">
                                <a class="page-link">Next</a>
                            </li>
                        }
                        else
                        {
                            <li class="page-item">
                                <a class="page-link" asp-action="Index" asp-route-ListingPage="@(Model.PagingInfo.CurrentPage + 1)">Next</a>
                            </li>
                        }
                    </ul>
                }
            </div>
        </div>
