    .tag-container {
        border: 2px solid #ccc;
        border-radius: 3px;
        background: #fff;
        display: flex;
        flex-wrap: wrap;
        align-content: flex-start;
        padding: 6px;
        font-weight: 500;
        font-size: 13px;
        color: black;
    }

    .tag {
        height: 30px;
        margin: 5px;
        padding: 5px 6px;
        /*border: 1px solid #03D392;
        border-radius: 3px;*/
        background: white;
        display: flex;
        align-items: center;
        color: #333;
        cursor: default;
        border-bottom: 2px solid red;
    }


        .tag i {
            font-size: 16px;
            color: #03D392;
            margin-left: 5px;
        }

    .tag-container input {
        padding: 5px;
        font-size: 16px;
        border: 0;
        outline: none;
        font-family: 'Rubik';
        color: #333;
        flex: 1;
    }

