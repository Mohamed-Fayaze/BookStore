    <!-- about -->

    <div class="about" id="about">
        <a asp-controller="Healthcare" asp-action="Detail" asp-route-Id="@Model.HealthcareBranch.HealthcareId">Go to Hospital</a>
        <div class="container">
            <div class="w3ls-heading">
                <h3>@Model.HealthcareBranch.Name</h3>
            </div>
            <div class="col-md-6 about-left">
                <div class="w3labout-img boxw3-agile">
                    @if (Model.HealthcareBranch.UploadImage != null)
                    {
                        <img src="@Model.HealthcareBranch.UploadImage" class="img-responsive" alt="Branch Photo">
                    }
                    else
                    {
                        <img src="~/images/dummyprofile/maleprofile.jpg" class="img-responsive" alt="Branch Photo">
                    }
                </div>
            </div>
            <div class="col-md-6 about-right">
                <h2>Contact</h2>
                <p><i class="fa fa-phone"></i> @Model.HealthcareBranch.ContactNo1</p>
                @if (Model.HealthcareBranch.ContactNo2 != null)
                {
                    <p><i class="fa fa-phone"></i> @Model.HealthcareBranch.ContactNo2</p>}
                <p><i class="fa fa-plus-square"></i> @Model.HealthcareBranch.EmergencyContactNo</p>
                <p><i class="fa fa-share-square-o"></i> @Model.HealthcareBranch.SocialMediaURL</p>
                <h2>Location</h2>
                <p>@Model.HealthcareBranch.Address</p>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
