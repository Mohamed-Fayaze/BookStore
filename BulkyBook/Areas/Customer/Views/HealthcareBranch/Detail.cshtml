                                break;
                            case "testing":
                                @Model.HealthcareBranch.TestingLabType.Name
                                break;
                            case "nursing":
                                @Model.HealthcareBranch.NursingType.Name
                                break;
                            case "ambulatory":
                                @Model.HealthcareBranch.AmbulatoryType.Name
                                break;
                            case "devices":
                                @Model.HealthcareBranch.MedicalSuppliesType.Name
                                break;
                            case "insurance":
                                @Model.HealthcareBranch.MedicalInsuranceType.Name
                                break;
                            case "pharmaceuticals":
                                @Model.HealthcareBranch.PharmaceuticalType.Name
                                break;
                        }
