                    </li>
                    @if (Model.HealthcareBranch.FacilityId != null)
                    {
                    <li class="list-group-item text-center resp-tab-item" aria-controls="tab_item-0" role="tab">
                        <div class="row">
                            <div class="col">
                                Facility
